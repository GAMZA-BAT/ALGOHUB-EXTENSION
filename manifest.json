{
    "manifest_version": 3,
    "name": "algohub",
    "version": "1.0",
    "description": "algohub-extension",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
      "http://localhost:8080/*",
      "https://www.acmicpc.net/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.acmicpc.net/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_icon": {
        "48": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png"
    },
    "web_accessible_resources": [
    {
      "resources": ["icon.png"],
      "matches": ["<all_urls>"]
    }
  ]
}